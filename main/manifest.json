{
  "name": "main",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    { "matches": ["https://www.amazon.com/*"], "js": ["index.js"] }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "./img/vue.png"
  },
  "icons": {
    "16": "./img/vue.png",
    "32": "./img/vue.png",
    "128": "./img/vue.png"
  }
}
